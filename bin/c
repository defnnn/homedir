#!/usr/bin/env bash

function main {
  local cmd="$1"; shift

  cue cmd \
    -t arg1="${1:-}" \
    -t arg2="${2:-}" \
    -t arg3="${3:-}" \
    -t arg4="${4:-}" \
    -t arg5="${5:-}" \
    -t arg6="${6:-}" \
    -t arg7="${7:-}" \
    -t arg8="${8:-}" \
    -t arg9="${9:-}" \
    "${cmd}"
}

main "$@"
